<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/  file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" label="obmenu-generator" execute="/usr/bin/obmenu-generator -p">      
  </menu>

  <menu id="extra-menu" title="Menu" label="Openbox 3">
	<item label="Terminal">
		<action name="Execute"><execute>alacritty</execute></action>
	</item>
	<item label="Emacs">
		<action name="Execute"><execute>emacs</execute></action>
	</item>
	<item label="Firefox">
		<action name="Execute"><execute>firefox</execute></action>
        </item>
	<item label="Firefox (_private)">
            <action name="Execute"><execute>firefox --private-window</execute></action>
	</item>
	<item label="Chromium">
		<action name="Execute"><execute>chromium</execute></action>
	</item>
	<item label="Chromium (_incognito)">
		<action name="Execute"><execute>chromium --incognito</execute></action>
	</item>
	<item label="Desktop">
		<action name="Execute"><execute>~/.config/openbox/bin/ob_desktop</execute></action>
	</item>
	<item label="Runâ€¦">
		<action name="Execute"><execute>~/.config/openbox/bin/ob_run</execute></action>
	</item>
	<separator label="System" />
	<menu id="system-menu"/>
	<separator />
  	<item label="Reconfigure">
      <action name="Reconfigure" />
  	</item>
    <item label="Restart">
      <action name="Restart" />
    </item>
	<item label="Log Out">
	  <action name="Exit">
	    <prompt>yes</prompt>
	  </action>
	</item>
    </menu>    
</openbox_menu>

